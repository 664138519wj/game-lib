package ${pack};

import com.isnowfox.core.net.message.Message;

import game.game.player.Player;

public interface MessageHandler<T extends Message> {
	/**
	 * 返回值决定是否把这个对象脱离对象缓冲！
	 * 
	 * @param msg
	 * @return 返回true 表示需要脱离缓冲，不然这个消息的内容可能被覆盖
	 */
	boolean handler(T msg, Player player);
}
